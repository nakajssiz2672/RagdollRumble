-- One-Shot Kill Script for Ragdoll Rumble

local player = game.Players.LocalPlayer

-- Function to auto-attack enemies for one-shot kills
local function autoAttack()
    local character = player.Character or player.CharacterAdded:Wait()
    local playerRootPart = character:WaitForChild("HumanoidRootPart")

    for _, enemy in ipairs(workspace:GetChildren()) do
        if enemy:IsA("Model") and enemy:FindFirstChild("Humanoid") and enemy ~= character then
            local enemyRootPart = enemy:FindFirstChild("HumanoidRootPart")
            if enemyRootPart and playerRootPart then
                local distance = (enemyRootPart.Position - playerRootPart.Position).magnitude
                if distance <= 50 then -- Adjust the range as necessary
                    enemy:FindFirstChild("Humanoid"):TakeDamage(9999) -- One-shot damage
                    print("One-shot enemy:", enemy.Name) -- Optional: Print to output
                end
            end
        end
    end
end

-- Start the auto-attack loop
while true do
    autoAttack()
    wait(1) -- Check every second
end
